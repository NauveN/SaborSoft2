@page
@model SaborSoft2.Pages.Usuarios.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].CorreoElectronico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].FechaRegistro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario[0].TipoUsuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Usuario) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorreoElectronico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaRegistro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoUsuario.TipoUsuarioId)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Cedula">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Cedula">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Cedula">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
